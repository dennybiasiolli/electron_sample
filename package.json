{
  "name": "electron_sample",
  "version": "0.1.0",
  "description": "converting a simple website to an electron app",
  "author": "Denny Biasiolli <denny.biasiolli@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "build-electron": "electron-packager . myFirstApp --platform=all --arch=all --overwrite --out=dist-desktop --icon 'icons/icon'",
    "start": "cross-env MY_BUILD='web' webpack-dev-server",
    "start-electron": "cross-env MY_BUILD='electron' npm run build && electron .",
    "pack": "build --config electron-builder.json --dir -mwl",
    "dist": "build --config electron-builder.json -mwl"
  },
  "repository": "git@github.com:dennybiasiolli/electron_sample.git",
  "main": "electron/main.js",
  "devDependencies": {
    "cross-env": "^5.0.0",
    "electron": "^1.6.10",
    "electron-builder": "^18.2.2",
    "electron-packager": "^8.7.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "electron-updater": "^1.16.0"
  }
}
